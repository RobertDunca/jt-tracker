<tr id="user-<%= user.id %>">
    <th scope="row"><%= user.id %></th>
    <td><%= user.email %></td>
    <td><%= user.username %></td>
    <td><%= user.role %></td>
    <td>
        <%= link_to 'Delete', user_path(user.id), method: :delete, remote: true, class: "delete_user" %>
        <%= link_to 'Update', edit_user_path(user.id) %>
    </td>
</tr>
